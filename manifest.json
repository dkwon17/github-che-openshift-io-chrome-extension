{
    "manifest_version": 2,
  
    "version": "3",
    "name": "Che on OpenShift.io Cloud IDE",
    "description": "Open GitHub projects on che.openshift.io.",
  
    "content_scripts": [ {
      "js":      [ "options.js", "script.js"  ],
      "matches": [ "https://github.com/*" ],
      "run_at":  "document_idle"
    } ],
    "permissions": [
      "storage"
    ],
    "options_ui": {
      "page": "options.html",
      "open_in_tab": false
    },
    "icons": {
      "32": "osio32.png"
    }
  }